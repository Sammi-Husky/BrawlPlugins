{
    "configurations": [
        {
            "name": "MWCC",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/lib/BrawlHeaders/Brawl/**",
                "${workspaceFolder}/lib/BrawlHeaders/hk410/**",
                "${workspaceFolder}/lib/BrawlHeaders/nw4r/**",
                "${workspaceFolder}/lib/BrawlHeaders/OpenRVL/**",
                "${workspaceFolder}/lib/BrawlHeaders/std/**",
                "${workspaceFolder}/lib/BrawlHeaders/utils/**",
                "${workspaceFolder}/lib/sy_core/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "windowsSdkVersion": "10.0.19041.0",
            "cStandard": "c99",
            "cppStandard": "c++98",
            "intelliSenseMode": "linux-clang-x64",
            "mergeConfigurations": false,
            "recursiveIncludes": {},
            "browse": {
                "limitSymbolsToIncludedHeaders": true
            },
            "compilerArgs": [
                "-nostdinc",
                "--fno-builtin",
                "--target=powerpc-eabi"
            ]
        }
    ],
    "version": 4
}